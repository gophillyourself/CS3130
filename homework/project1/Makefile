final:
	#Part A
	g++ project1_parta.cpp fibonnaci.cpp -o fibPartA
	cat project1_parta.cpp > project1_parta.txt

	echo "\n" >> project1_parta.txt
	echo "---Part A Output---" >> project1_parta.txt
	./fibPartA 100 >> project1_parta.txt
	cat project1_parta.txt

partA:
	g++ project1_parta.cpp fibonnaci.cpp -o fibPartA
	./fibPartA 15

